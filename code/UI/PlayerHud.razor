@using Sandbox;
@using Sandbox.UI;
@namespace Scenebox.UI
@inherits PanelComponent

<root>
	@if (!Player.Local.IsValid()) return;

	<NotificationPanel />

	<InventoryBar />

	<PlayerVitals />

	<HurtOverlay />
</root>

@code
{
	protected override int BuildHash() => System.HashCode.Combine(Player.Local.IsValid());
}